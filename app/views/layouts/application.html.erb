<!DOCTYPE html>
<html>
<head>
  <title>CryptoWallet</title>
  <%= csrf_meta_tags %>
  <%= csp_meta_tag %>

  <!--  import CSS-->
  <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
  <%= stylesheet_link_tag 'scaffolds', media: 'all', 'data-turbolinks-track': 'reload' %>
  <%= stylesheet_link_tag params[:controller], media: 'all', 'data-turbolinks-track': 'reload' %>

  <!--  import JavaScript-->
  <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  <%= javascript_include_tag params[:controller], 'data-turbolinks-track': 'reload' %>
</head>

<body>
<h1 class="text_violeta">Cabeçalho público - <%= params[:controller] %></h1>

<fieldset>
  <%= yield %>
</fieldset>

<h1 class="text_violeta">Rodapé público</h1>
</body>
</html>
